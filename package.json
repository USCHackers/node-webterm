{
  "name": "webterm",
  "version": "1.0.0",
  "description": "A simple webterminal using terminal.js and child_pty",
  "scripts": {
    "build": "browserify -t browserify-nodent main.js -o public/main.js",
    "build:dev": "watchify -t browserify-nodent main.js -o public/main.js",
    "test": "jshint app.js public",
    "start": "DEBUG='*' node app.js",
    "start:dev": "DEBUG='*' nodemon app.js"
  },
  "author": "Enno Boland <eb@s01.de>",
  "license": "MIT",
  "dependencies": {
    "browserify": "^14.4.0",
    "browserify-nodent": "^1.0.22",
    "child_pty": "^3.0.0",
    "express": "^4.15.4",
    "socket.io": "^1.4.5",
    "socket.io-stream": "^0.9.0",
    "terminal.js": "^1.0.3"
  },
  "jshintConfig": {
    "eqeqeq": true,
    "quotmark": true,
    "nonew": true,
    "indent": true,
    "latedef": true,
    "noarg": true,
    "node": true
  },
  "devDependencies": {
    "jshint": "^2.5.10",
    "nodemon": "^1.12.0",
    "watchify": "^3.9.0"
  }
}
